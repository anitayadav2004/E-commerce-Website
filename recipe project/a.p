*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Poppins', sans-serif;
}
.container{
    height: 100vh;
    width: 100vh;
    
    overflow-x:none;
}
.navcontainer{
    
    width: 100vw;
    background-color: aliceblue;
    min-height: 80px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
}
nav{
    width: 100%;
}
#navlist{
    width: 100%;
    padding: 15px;
    /*margin-left:30px;
    margin-right:30px ;*/
    list-style:none;
    font-size: 2.3rem;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
#navlogo{
    font-size: 2.3rem;
    color: black;
    
}
a{
    text-decoration: none;
}
#navitems{
    display: flex;
    justify-content: space-between;
    align-items: center;

}
